{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
		"no-param-reassign": 0,
		"no-console": 0,
		"func-names": 0,
		"no-tabs": 0,
		"no-continue": 0,
		"no-plusplus": 0,
		"eqeqeq": 0,
		"no-loop-func": 0,
		"no-return-assign": 0,
		"max-len": 0,
		"no-restricted-properties": 0,
		"import/no-extraneous-dependencies": 0,
		"no-mixed-operators": 0,
		"no-underscore-dangle": 0,
		"guard-for-in": 0,
		"no-restricted-syntax": 0,
		"no-restricted-globals": 0,
		"no-alert": 0,
		"no-void": 0,
		"no-unused-vars": 0,
		"no-lonely-if": 0,
		"indent": [2, "tab"]
    }
}
